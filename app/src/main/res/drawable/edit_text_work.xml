<selector xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
            <stroke android:color="@color/create_order_a" android:width="2dp" />
            <corners
                android:topLeftRadius="16dp"
                android:bottomLeftRadius="16dp"
                android:bottomRightRadius="16dp"
                android:topRightRadius="16dp"
                tools:ignore="StateListReachable" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" tools:ignore="StateListReachable" />
            <stroke android:color="@color/create_order_b" android:width="1dp" />
            <corners
                android:topLeftRadius="16dp"
                android:bottomLeftRadius="16dp"
                android:bottomRightRadius="16dp"
                android:topRightRadius="16dp"
                tools:ignore="StateListReachable" />
        </shape>

    </item>

</selector>